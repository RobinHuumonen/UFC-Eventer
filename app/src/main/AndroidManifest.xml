<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ufceventer">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_own"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_own"
        android:supportsRtl="true"
        android:theme="@style/Theme.UFCEventer">
        <activity android:name=".FightsActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".PickAndScoreActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".ViewPicksActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>